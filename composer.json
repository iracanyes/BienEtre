        "symfony/polyfill-apcu": "~1.0",
        "symfony/polyfill-mbstring": "~1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/webpack-encore-pack": "^1.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "doctrine/cache": "~1.0",
        "hautelook/alice-bundle": "dev-master",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "nelmio/alice": "3.1.3",
        "symfony/browser-kit": "~4.0",
        "symfony/console": "^3.3",
        "symfony/css-selector": "~4.0",
        "symfony/debug": "^3.4",
        "symfony/debug-bundle": "^3.4",
        "symfony/dom-crawler": "~4.0",
        "symfony/dotenv": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/polyfill-intl-icu": "~1.0",
        "symfony/property-info": "~4.0",
        "symfony/requirements-checker": "^1.0",
        "symfony/templating": "~4.0",
        "symfony/translation": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^4.0",
        "symfony/var-dumper": "^3.4",
        "symfony/web-profiler-bundle": "^3.4",
        "symfony/web-server-bundle": "^3.4",
        "symfony/workflow": "^3.4",
        "symfony/yaml": "^3.4",
        "twig/extensions": "^1.5",
        "theofidry/alice-data-fixtures": "1.0.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "requirements-checker": "script",
            "php bin/console cache:warmup": "script"

        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BVFR7CR0KSWKXK78SRASPXNF",
            "allow-contrib": true
        }
    }
}
