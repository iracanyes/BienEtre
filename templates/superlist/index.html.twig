                                                            {# Boucle sur les localités #}
                                                            {% for locality in localities %}
                                                                <option>{{ locality.locality }}</option>
                                                            {% endfor %}
                                                        </select>
                                                    </div><!-- /.form-group -->

                                                    <div class="hero-image-category form-group">
                                                        <select class="form-control" title="Commune">
                                                            {% for township in townships %}
                                                                <option value="">{{ township.township }}</option>
                                                            {% endfor %}
                                                        </select>
                                                    </div><!-- /.form-group -->
                                                    <div class="hero-image-location form-group">
                                                        <select class="form-control" title="Commune">
                                                            {% for postalCode in postalCodes %}
                                                                <option value="">{{ postalCode.postalCode }}</option>
                                                            {% endfor %}
                                                        </select>
                                                    </div><!-- /.form-group -->

                                                    <div class="hero-image-price form-group">
                                                        <input type="text" class="form-control" placeholder="Service">
                                                    </div><!-- /.form-group -->

                                                    <button type="submit" class="btn btn-primary btn-block">Lancer la recherche</button>
